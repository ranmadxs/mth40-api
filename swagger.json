  "paths": {
    "/armies/list": {
      "get": {
        "tags": [
          "army-factions"
        ],
        "summary": "Lista de Conferencias de Ejércitos",
        "operationId": "armiesList",
        "description": "Obtiene la lista de las conferencias de los Ejércitos de WH40K, por ejemplo, Imperium, Chaos, Xenos.\n",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "Resultado de la lista de conferencias",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Army"
              }
            }
          },
          "400": {
            "description": "Mala petición"
          }
        }
      }
    }
  },
  "definitions": {
    "Army": {
      "type": "object",
      "required": [
        "ID",
        "NAME",
        "CODE"
      ],
      "properties": {
        "ID": {
          "type": "integer",
          "example": 1
        },
        "NAME": {
          "type": "string",
          "example": "Xenos"
        },
        "CODE": {
          "type": "string",
          "example": "XEN"
        }
      }
    }
